package com.ike.model;

import java.util.Collection;
import java.util.Iterator;
import com.ike.model.DAOException;
import com.ike.model.to.DetalleExpediente;

/*
 * @author velazqrb
 */
public class DAOExpediente extends com.ike.model.DAOBASE {

    /* Creates a new instance of DAOExpediente */
    public DAOExpediente() {
    }

    public DetalleExpediente getExpediente(String strclExpediente) throws DAOException {

        StringBuffer sb = new StringBuffer();
        Collection col = null;


        sb.append("st_DAODetalleExpediente ").append(strclExpediente);
        col = this.rsSQLNP(sb.toString(), new DAOExpedienteFiller());

        Iterator it = col.iterator();
        return it.hasNext() ? (DetalleExpediente) it.next() : null;
    }

    public class DAOExpedienteFiller implements com.ike.model.LlenaDatos {

        public Object llena(java.sql.ResultSet rs) throws java.sql.SQLException {


            DetalleExpediente Expediente = new DetalleExpediente();

            Expediente.setclExpediente(rs.getString("clExpediente"));
            Expediente.setNuestroUsuario(rs.getString("NuestroUsuario"));
            Expediente.setContacto(rs.getString("Contacto"));
            Expediente.setClave(rs.getString("Clave"));
            Expediente.setNombre(rs.getString("Nombre"));
            Expediente.setFechaApertura(rs.getString("FechaApertura"));
            Expediente.setclCuenta(rs.getString("clCuenta"));
            Expediente.setFechaRegistro(rs.getString("FechaRegistro"));
            Expediente.setTelefono1(rs.getString("Telefono1"));
            Expediente.setTelefono2(rs.getString("Telefono2"));
            Expediente.setFechaSiniestro(rs.getString("FechaSiniestro"));
            Expediente.setDescripcionOcurrido(rs.getString("DescripcionOcurrido"));
            Expediente.setLada1(rs.getString("Lada1"));
            Expediente.setLada2(rs.getString("Lada2"));
            Expediente.setdsServicio(rs.getString("dsServicio"));
            Expediente.setdsSubservicio(rs.getString("dsSubservicio"));
            Expediente.setdsEntFed(rs.getString("dsEntFed"));
            Expediente.setdsMunDel(rs.getString("dsMunDel"));
            Expediente.setdsEstatus(rs.getString("dsEstatus"));
