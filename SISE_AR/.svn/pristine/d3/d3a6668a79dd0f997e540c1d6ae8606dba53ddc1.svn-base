<%@ page contentType="text/html; charset=iso-8859-1" language="java" import="java.sql.ResultSet,Utilerias.UtileriasBDF,Seguridad.SeguridadC" errorPage="" %>
<%@page pageEncoding="iso-8859-1"%>
<html>
    <head><title>JSP Page</title>
        <link href="../../StyleClasses/Global.css" rel="stylesheet" type="text/css">
        <style>
            .Titulo {font-family: Verdana, Arial, Helvetica, sans-serif; font-size: 12px; font-weight: bold; color: #000066; text-transform: uppercase; text-align:center;}
        </style>
    </head>
    <body class="cssBody">
        <!-- SOLO EXPEDIENTES VIALES SERÁN PUBLICADOS ES DECIR EL CLSERVICIO 1-->
        <%
            String StrclServicio = "";
            String StrclSubServicio = "";
            String StrclExpediente = "";
            String StrPermiteWS = "";

            if (session.getAttribute("clExpediente") != null) {
                StrclExpediente = session.getAttribute("clExpediente").toString();
            }

            if (session.getAttribute("clServicio") != null) {
                StrclServicio = session.getAttribute("clServicio").toString();
            }
            
            if (session.getAttribute("clSubServicio") != null) {
                StrclSubServicio = session.getAttribute("clSubServicio").toString();
            }

            ResultSet rsValidaWS = UtileriasBDF.rsSQLNP("st_WSValidaExpediente " + StrclExpediente);

            if (rsValidaWS.next()) {
                StrPermiteWS = rsValidaWS.getString("PermiteWS");
            } else {
                StrPermiteWS = "0";
            }

            System.out.println("WS 0");
        %>
        <% if (StrclServicio.equalsIgnoreCase("1") && StrPermiteWS.equalsIgnoreCase("1")) {%>

    <center style="position:relative; top:90px;">
        <p class='Titulo'>Realizar Publicación Automática?<br></p>
        <table align="center">
            <tr>
                <td width="200" align="center">
                    <IMG alt='Asignación Automática' SRC='../../Imagenes/accept.png' onClick='fnPorPublicacion();' WIDTH = 64 HEIGHT = 64>
                </td>
                <td width="200" align="center">
                    <IMG alt='Asignación Manual' SRC='../../Imagenes/remove.png' onClick='fnAsigManual();' WIDTH = 64 HEIGHT = 64>
                </td>
            </tr>
        </table>
    </center>

    <% } else {%>
    <script> location.href = '../../servlet/Utilerias.Lista?P=184&Apartado=S';</script>
    <% }%>

    <script>
        function fnAsigManual(){
        //System.out.println("LA ASIGNACION SERA MANUAL...");
        location.href='../../servlet/Utilerias.Lista?P=184&Apartado=S';
        }

        function fnPorPublicacion(){
        //System.out.println("LA ASIGNACION SERA AUTOMATICA...");
        window.open('WSEnviaExpediente.jsp?&clExpediente=<%=StrclExpediente%>','','resizable=no,menubar=0,status=0,toolbar=0,height=50,width=50,screenX=-50,screenY=0,scrollbars=yes')
        //location.href='../../Utilerias/Lista.jsp?P=184&Apartado=S';
        location.href='../../servlet/Utilerias.Lista?P=184&Apartado=S';
        //alert('Publicado :3');
        }
    </script>
</body>
</html>