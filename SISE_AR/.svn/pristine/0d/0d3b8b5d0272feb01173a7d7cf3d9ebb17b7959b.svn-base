package com.ike.concierge.to;

/*
 *
 * @author rfernandez
 */
public class CSPasajero {


    private String clAsistencia;
    private String clPasajero;
    private String Nombre;
    private String Edad;
    private String TipoBoleto;
    private String ClaveConf;
    private String Costo;


    /* Creates a new instance of ConciergeWow */
    public CSPasajero() {
    }

    public String getclAsistencia() {
        return clAsistencia;
    }

    public void setclAsistencia(String clAsistencia) {
        this.clAsistencia = clAsistencia;
    }

    public String getclPasajero() {
        return clPasajero;
    }

    public void setclPasajero(String clPasajero) {
        this.clPasajero = clPasajero;
    }

    public String getNombre() {
        return Nombre;
    }

    public void setNombre(String Nombre) {
        this.Nombre = Nombre;
    }

    public String getEdad() {
        return Edad;
    }

    public void setEdad(String Edad) {
        this.Edad = Edad;
    }

    public String getTipoBoleto() {
        return TipoBoleto;
    }

    public void setTipoBoleto(String TipoBoleto) {
        this.TipoBoleto = TipoBoleto;
    }

      public String getClaveConf() {
        return ClaveConf;
    }

    public void setClaveConf(String ClaveConf) {
        this.ClaveConf = ClaveConf;
    }

    public String getCosto() {
        return Costo;
    }

    public void setCosto(String Costo) {
        this.Costo = Costo;
    }
}
